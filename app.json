{
  "expo": {
    "name": "Co-Ze_BETAVERSION",
    "slug": "Co-Ze_BETAVERSION",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "scheme": "cozy",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "facebookScheme": "fb321336776685083",
    "assetBundlePatterns": [
      "**/*"
    ],
        "plugins": [
      [
        "expo-facebook",
        {
          "userTrackingPermission": false
        }
      ]
    ],
    "ios": {
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Family Members know where you are"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.CoZy.CoZyFamilies",
      "buildNumber": "1.0.0",
      "usesAppleSignIn": true
    },
    "android": {
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
          "package": "com.CoZy.CoZyFamilies",
      "versionCode": 1
    },
    "web": {
    }
  }
}
